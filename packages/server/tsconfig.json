{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "outDir": "./dist",
    "noStrictGenericChecks": true,
    "ignoreDeprecations": "5.0"
  },
  "include": ["src/**/*"]
}
